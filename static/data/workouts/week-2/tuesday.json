  "mobility_preparation": {
    "title": "Mobility Prep",
    "duration": "08:30‚Äì08:50",
    "detailed_title": "Shoulder and Wrist Preparation Instructions",
    "components": [
      {
        "name": "Shoulder Circles",
        "instructions": [
          {
            "phase": "SETUP",
            "instruction": "Stand with arms at sides, relaxed shoulder position"
          },
          {
            "phase": "SMALL CIRCLES",
            "instruction": "Start with 10 small circles forward, 10 backward"
          },
          {
            "phase": "LARGE CIRCLES",
            "instruction": "Progress to 10 large arm circles each direction"
          },
          {
            "phase": "CONTROL",
            "instruction": "Smooth, controlled movement, feel shoulder joint warming up"
          },
          {
            "phase": "PURPOSE",
            "instruction": "Increases blood flow and range of motion for serves"
          }
        ]
      },
      {
        "name": "Wrist Circles",
        "instructions": [
          {
            "phase": "POSITION",
            "instruction": "Extend arms forward, make fists with both hands"
          },
          {
            "phase": "CIRCLES",
            "instruction": "10 circles each direction with both wrists simultaneously"
          },
          {
            "phase": "RANGE",
            "instruction": "Full range of motion, feel wrist joint loosening"
          },
          {
            "phase": "PURPOSE",
            "instruction": "Prepares wrists for racquet impact and control"
          }
        ]
      },
      {
        "name": "Band Stretches",
        "exercises": [
          {
            "type": "SHOULDER DISLOCATES",
            "instruction": "Hold band wide, slowly rotate arms overhead and behind back"
          },
          {
            "type": "EXTERNAL ROTATIONS",
            "instruction": "Elbow at 90¬∞, rotate forearm out against band resistance"
          },
          {
            "type": "INTERNAL ROTATIONS",
            "instruction": "Rotate forearm in against band resistance"
          },
          {
            "type": "REPS",
            "instruction": "10-12 each movement, focus on smooth controlled motion"
          },
          {
            "type": "PURPOSE",
            "instruction": "Activates rotator cuff muscles before tennis practice"
          }
        ]
      }
    ]
  },
