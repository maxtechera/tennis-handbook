        "name": "Band Pull-aparts",
        "sets": "3",
        "reps": "15",
        "load": "Medium resistance band",
        "rest_seconds": 60,
        "detailed_title": "Consistent Tension Instructions",
        "technique_points": [
          {
            "phase": "SETUP",
            "instruction": "Hold resistance band with both hands, arms extended in front at chest height"
          },
          {
            "phase": "GRIP",
            "instruction": "Hands shoulder-width apart on band, palms facing down"
          },
          {
            "phase": "PULL",
            "instruction": "Pull band apart by squeezing shoulder blades together"
          },
          {
            "phase": "PEAK",
            "instruction": "Pull until band touches chest, feel work in rear delts and rhomboids"
          },
          {
            "phase": "TENSION",
            "instruction": "Maintain constant tension in band throughout entire movement"
          },
          {
            "phase": "RETURN",
            "instruction": "Slowly return to start position, resist band's pull"
          },
          {
            "phase": "POSTURE",
            "instruction": "Keep shoulders down, don't shrug up during movement"
          },
          {
            "phase": "BREATHING",
            "instruction": "Exhale during pull apart, inhale during return"
          },
          {
            "phase": "TENNIS APPLICATION",
            "instruction": "Counters forward shoulder posture from tennis stance"
          },
          {
            "phase": "PROGRESSION",
            "instruction": "Use thicker band or wider grip when current becomes easy"
          }
