    "main_exercises": [
      {
        "name": "Incline Dumbbell Press",
        "sets": 3,
        "reps": 8,
        "rest_seconds": 120,
        "load": "Select weight for 8 rep max",
        "tempo": "3-1-X-1",
        "protocol": "Controlled full range of motion",
        "tennis_application": "Builds upper body power for overhead serves and volleys",
        "detailed_instructions": {
          "title": "Controlled Full Range of Motion Instructions",
          "technique_points": [
            {
              "phase": "SETUP",
              "instruction": "Set incline bench to 30-45 degrees, sit with dumbbells resting on thighs"
            },
            {
              "phase": "STARTING POSITION",
              "instruction": "Lie back and position dumbbells at chest level, feet flat on floor"
            },
            {
              "phase": "GRIP",
              "instruction": "Hold dumbbells with palms facing forward, wrists straight and strong"
            },
            {
              "phase": "DESCENT",
              "instruction": "Lower weights slowly over 3 seconds, feel stretch in chest muscles"
            },
            {
              "phase": "BOTTOM POSITION",
              "instruction": "Lower until dumbbells are at chest level, elbows slightly below shoulders"
            },
            {
              "phase": "PRESS",
              "instruction": "Drive weights up and slightly together, focusing on chest muscle contraction"
            },
            {
              "phase": "BREATHING",
              "instruction": "Inhale during descent, exhale forcefully during pressing phase"
            },
            {
              "phase": "PROGRESSION",
              "instruction": "Increase weight when you can complete all reps with perfect form"
            }
          ]
        }
      },
      {
        "name": "Lat Pulldowns",
        "sets": 3,
        "reps": 10,
        "rest_seconds": 90,
        "load": "Focus on lat engagement over heavy weight",
        "tempo": "2-1-3-1",
        "protocol": "Lat engagement focus",
        "tennis_application": "Strengthens pulling muscles for groundstroke power",
        "detailed_instructions": {
          "title": "Lat Engagement Focus Instructions",
          "technique_points": [
            {
              "phase": "SETUP",
              "instruction": "Sit at lat pulldown machine, adjust thigh pads to secure legs"
            },
            {
              "phase": "GRIP",
              "instruction": "Use wide overhand grip, hands wider than shoulders on bar"
            },
            {
              "phase": "STARTING POSITION",
              "instruction": "Sit tall with slight lean back, chest up, shoulders down"
            },
            {
              "phase": "PULL PHASE",
              "instruction": "Pull bar down to upper chest, leading with elbows not hands"
            },
            {
              "phase": "LAT FOCUS",
              "instruction": "Think 'pull elbows back and down' to target lats specifically"
            },
            {
              "phase": "SQUEEZE",
              "instruction": "Hold peak contraction for 1 second, feel lats working"
            },
            {
              "phase": "RETURN",
              "instruction": "Slowly return bar to start over 3 seconds, maintain tension"
            },
            {
              "phase": "AVOID",
              "instruction": "Don't lean back excessively or use momentum to move weight"
            },
            {
              "phase": "PROGRESSION",
              "instruction": "Increase weight when you can feel lats working throughout full range"
            }
          ]
        }
      },
      {
        "name": "Band Pull-Aparts",
        "sets": 3,
        "reps": 15,
        "rest_seconds": 60,
        "load": "Medium resistance band",
        "tempo": "1-1-2-1",
        "protocol": "Consistent tension maintenance",
        "tennis_application": "Posterior deltoid strength for racket control",
        "detailed_instructions": {
          "title": "Consistent Tension Instructions",
          "technique_points": [
            {
              "phase": "SETUP",
              "instruction": "Hold resistance band with arms extended at chest height"
            },
            {
              "phase": "GRIP",
              "instruction": "Hands shoulder-width apart, palms facing down"
            },
            {
              "phase": "PULL APART",
              "instruction": "Pull band apart by squeezing shoulder blades together"
            },
            {
              "phase": "PEAK CONTRACTION",
              "instruction": "Hold for 1 second, feel rear delts and rhomboids working"
            },
            {
              "phase": "RETURN",
              "instruction": "Slowly return to start position over 2 seconds"
            },
            {
              "phase": "TENSION",
              "instruction": "Maintain band tension throughout entire range of motion"
            },
            {
              "phase": "FOCUS",
              "instruction": "Quality over quantity - feel the muscles working"
            },
            {
              "phase": "PROGRESSION",
              "instruction": "Increase reps or use stronger band when current becomes easy"
            }
          ]
        }
      }
    ]
  },
