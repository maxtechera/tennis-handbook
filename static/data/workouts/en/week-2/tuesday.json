  "strength_training": {
    "title": "Upper Body Strength Development",
    "timing": "7:30-8:15",
    "duration": "45 minutes",
    "description": "Tennis-specific upper body strength training",
    "attribution": {
      "source": "Tennis-specific upper body training",
      "methodology": "Upper body power development for tennis",
      "validation": "Tennis strength training protocols",
      "elite_use": "Professional tennis strength training"
